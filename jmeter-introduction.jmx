          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">//do some log transformation here
//insert total request time at the end
def logfile = new File(props.get(&quot;logfile&quot;))
def results = logfile.readLines().findAll{
	line -&gt; !line.contains(&quot;Request&quot;)
}.sum{ 
	line -&gt; line.split(&quot;,&quot;)[1] as int //request time, &quot;as&quot; to be treated as numbers not Strings
}
log.info &quot;Results: $results&quot;
logfile.append(&quot;\nTotal: ${results} ms&quot;)
</stringProp>

                <stringProp name="filename"></stringProp>
                <stringProp name="parameters"></stringProp>
                <stringProp name="script">def threadNum = ctx.getThreadNum() as int
log.info(&quot;URL ${vars.get(&apos;URL&apos;)}, threadNum: ${ctx.getThreadNum()}&quot;)
log.info &quot;Loop iteration: &quot; + vars.getIteration()
//change domain for every second thread
if( (threadNum % 2) == 0 ){
	 vars.put(&apos;URL&apos;,vars.get(&apos;alternativeURL&apos;))
	 log.info(&quot;URL changed to: ${vars.get(&apos;URL&apos;)}&quot;)
}
</stringProp>
